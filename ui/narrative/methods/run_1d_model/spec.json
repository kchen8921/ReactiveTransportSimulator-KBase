{
    "name": "run_batch_model",
    "ver": "0.0.1",
    "authors": [
        "kchen"
    ],
    "contact": "http://kbase.us/contact-us/",
    "visible": true,
    "categories": [
        "active",
        "reactive_transport"
    ],
    "widgets": {
        "output": "no-display",
        "input": null
    },
    "parameters": [{
            "id": "input_FBA_model",
            "default_values": [""],
            "advanced": false,
            "field_type": "text",
            "optional": true,
            "text_options": {
                "valid_ws_types": [
                    "KBaseFBA.FBAModel"
                ]
            }
        },
        {
            "id": "input_initial_boundary_condition",
            "optional": true,
            "advanced": false,
            "allow_multiple": false,
            "default_values": [""],
            "field_type": "dynamic_dropdown",
            "dynamic_dropdown_options": {
                "data_source": "ftp_staging"
            }
        },
        {
            "id": "number_simulated_reactions",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["2"],
            "field_type": "text"
        },
        {
            "id": "velocity",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["10"],
            "field_type": "text"
        },
        {
            "id": "length",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["1"],
            "field_type": "text"
        },
        {
            "id": "number_grids",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["100"],
            "field_type": "text"
        },
        {
            "id": "simulation_time",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["1"],
            "field_type": "text"
        },
        {
            "id": "snapshot_period",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["0.1"],
            "field_type": "text"
        },
        {
            "id": "temperature",
            "optional": false,
            "advanced": false,
            "allow_multiple": false,
            "default_values": ["25"],
            "field_type": "text"
        }
    ],
    "behavior": {
        "service-mapping": {
            "url": "",
            "name": "ReactiveTransportSimulator",
            "method": "run_1d_model",
            "input_mapping": [{
                    "target_property": "workspace",
                    "narrative_system_variable": "workspace"
                },
                {
                    "input_parameter": "input_FBA_model",
                    "target_property": "input_FBA_model",
                    "target_type_transform": "resolved-ref"
                },
                {
                    "input_parameter": "input_initial_boundary_condition",
                    "target_property": "input_initial_boundary_condition"
                },
                {
                    "input_parameter": "number_simulated_reactions",
                    "target_property": "number_simulated_reactions"
                },
                {
                    "input_parameter": "velocity",
                    "target_property": "velocity"
                },
                {
                    "input_parameter": "length",
                    "target_property": "length"
                },
                {
                    "input_parameter": "number_grids",
                    "target_property": "number_grids"
                },
                {
                    "input_parameter": "simulation_time",
                    "target_property": "simulation_time"
                },
                {
                    "input_parameter": "snapshot_period",
                    "target_property": "snapshot_period"
                },
                {
                    "input_parameter": "temperature",
                    "target_property": "temperature"
                }
            ],
            "output_mapping": [{
                "service_method_output_path": [0, "report_name"],
                "target_property": "report_name"
            }, {
                "service_method_output_path": [0, "report_ref"],
                "target_property": "report_ref"
            }, {
                "narrative_system_variable": "workspace",
                "target_property": "workspace_name"
            }]
        }
    },
    "job_id_output_field": "docker"
}